    "multiple environment"
  ],
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@radix-ui/react-popover": "^1.0.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
